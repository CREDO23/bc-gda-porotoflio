{
  "name": "bc-gda-portfolio",
  "version": "1.0.0",
  "description": "An E-market that allows people to create virtuals shops and sell products",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:prod": "node dist/index.js",
    "build": "yarn install && tsc",
    "test": "docker-compose up --build --renew-anon-volumes --abort-on-container-exit && docker-compose down",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint --fix src/**/*.ts",
    "prepare-husky": "npx husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CREDO23/bc-gda-portfolio.git"
  },
  "keywords": [
    "E-market",
    "E-commerce"
  ],
  "author": "Credo23",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CREDO23/bc-gda-portfolio/issues"
  },
  "homepage": "https://github.com/CREDO23/bc-gda-portfolio#readme",
  "dependencies": {
    "@types/nodemailer": "^6.4.8",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^7.0.7",
    "express-jwt": "^8.4.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.3",
    "nodemailer-express-handlebars": "^6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.2",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
